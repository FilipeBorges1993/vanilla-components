{
  "name": "@indigit/vanilla-components",
  "version": "0.6.20",
  "description": "ðŸ¤Œ A lightweight Vue 3 Component Library based on VariantJS & Headless UI",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/igerslike/vanilla-components"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "author": "Jon <support@indigit.pt>",
  "license": "MIT",
  "engines": {
    "node": ">=10"
  },
  "sideEffects": false,
  "private": false,
  "files": [
    "dist/**/*"
  ],
  "types": "./dist/index.d.ts",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "scripts": {
    "lib:build": "vite build",
    "lib:watch-old": "vite build --watch",
    "lib:publish": "npm publish --access public",
    "watch:lib": "nodemon --exec 'yarn lib:build'",
    "watch:both": "concurrently \"yarn && yarn watch:lib\" \"yarn && yarn watch:docs\"",
    "watch:docs": "yarn --cwd \"docs\" dev",
    "analyze": "vite build --config analyze.config.js",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "format": "prettier .  --write"
  },
  "dependencies": {
    "@headlessui/vue": "^1.5.0",
    "@heroicons/vue": "^1.0.1",
    "@popperjs/core": "^2.11.2",
    "@variantjs/vue": "^0.0.21",
    "@vueuse/core": "^8.1.2",
    "axios": "^0.26.1",
    "crypto-js": "^4.1.1",
    "flatpickr": "^4.6.9",
    "fuse.js": "^6.4.6",
    "libphonenumber-js": "^1.9.49",
    "moment-timezone": "^0.5.34",
    "vue": "^3.2.31"
  },
  "peerDependencies": {
    "@headlessui/vue": "^1.5.0",
    "@heroicons/vue": "^1.0.1",
    "@popperjs/core": "^2.11.2",
    "@variantjs/core": "^0.0.87",
    "@variantjs/vue": "^0.0.21",
    "body-scroll-lock": "^4.0.0-beta.0",
    "flatpickr": "^4.6.9",
    "fuse.js": "^6.4.6",
    "libphonenumber-js": "^1.9.49",
    "lodash": "^4.17.21",
    "moment-timezone": "^0.5.34",
    "vue": "^3.2.31"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/crypto-js": "^4.1.1",
    "@types/lodash": "^4.14.178",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "@vitejs/plugin-vue": "^2.1.0",
    "@vue/compiler-sfc": "^3.1.5",
    "eslint": "^8.7.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-tree-shaking": "^1.9.2",
    "eslint-plugin-vue": "^8.3.0",
    "prettier": "^2.3.2",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-visualizer": "^5.5.2",
    "typescript": "^4.5.4",
    "vite": "^2.4.3",
    "vite-plugin-dts": "^0.9.9"
  }
}
